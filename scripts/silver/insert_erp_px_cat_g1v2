select id
,cat
,subcat
,maintenance
from bronze.erp_px_cat_g1v2


---check sapce
select
*
from bronze.erp_px_cat_g1v2 where cat != trim(cat) or subcat != (subcat) or maintenance != (maintenance)

---check consist.
select
distinct cat
from bronze.erp_px_cat_g1v2

---insert into
truncate table silver.erp_px_cat_g1v2;

insert into silver.erp_px_cat_g1v2(
       [id]
      ,[cat]
      ,[subcat]
      ,[maintenance]
)
select id
,cat
,subcat
,maintenance
from bronze.erp_px_cat_g1v2


---check overview
select * from silver.erp_px_cat_g1v2
